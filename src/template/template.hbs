<li class="list-item" id="{{id}}">
    <h2 class="title">{{name}}</h2>
    <img class="img-car" src="{{img}}" alt="">
    <p class="descr">{{description}}</p>
    <p class="price">price: {{price}}</p>
    <ul>
        {{#configuration}}
        <li>{{this}}</li>
        {{/configuration}}
    </ul>
</li>